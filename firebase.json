{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/summarizeFileContent",
        "function": "apiSummarizeFileContent"
      },
      {
        "source": "/api/enrichKeywords",
        "function": "apiEnrichKeywords"
      },
      {
        "source": "/api/extractTextFromDocument",
        "function": "apiExtractTextFromDocument"
      },
      {
        "source": "/api/extractKeywordValues",
        "function": "apiExtractKeywordValues"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "runtime": "nodejs20"
    }
  ]
}
